    function voteFor(id)
    {
            this.id = id;
            var url = "comment/?action=vote";
            var data = '&id='+id + '&field=support';
            $.ajax({
                url: url,
                data: data,
                type: 'post',
                success :function (msg){
                    $('#count_'+ id+'_1').text(msg);
                }
            });
        }
    /**
    * 反对
    **/
    function voteAgainst (id )
    {
        this.id = id;
        var url  = "comment/?action=vote";
        var data = '&id='+id + '&field=against';
        $.ajax({
            url: url,
            data: data,
            type: 'post',
            success :function (msg){
                $('#count_'+ id +'_0').text(msg);
            }
        });
    }
    
